<?xml version='1.0' encoding='utf-8'?>
<widget id="im.zego.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ZegoWebRTC</name>
    <description>
        webview support test by zego
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        zego front team
    </author>
    <content src="https://bansheehannibal.github.io/webrtcDemo/?platform=cordova" />
    <access origin="*" />
    <allow-navigation href="https://*/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-android-permissions" spec="^1.0.0" />
</widget>
